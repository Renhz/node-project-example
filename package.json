{
  "name": "node-project-example",
  "version": "1.0.0",
  "description": "node app project example in typescript with package manager, dockerfile, ci script, semantic-release and lint set",
  "main": "index.js",
  "scripts": {
    "develop": "DEBUG='*,-nodemon*,-mqtt-packet:*' nodemon src/index.ts",
    "build": "rm -rf dist && tsc -p tsconfig.prod.json",
    "start": "node -r ./tsconfig-paths-bootstrap.js dist/index.js",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Renhz/node-project-example.git"
  },
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/Renhz/node-project-example/issues"
  },
  "homepage": "https://github.com/Renhz/node-project-example#readme",
  "dependencies": {
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.1",
    "@types/debug": "^4.1.12",
    "@types/jest": "^27.5.2",
    "@types/node": "^14.18.63",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-universe": "^7.0.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "nock": "^13.3.8",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "semantic-release": "^17.4.7",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
